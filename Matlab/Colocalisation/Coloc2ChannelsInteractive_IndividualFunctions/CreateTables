% MakeNewFigure related functions
function [] = CreateTables(f)
    % table current data
    ColNames = {'ch1' 'ch2' 'PCC' 'M1' 'M2' '% ch1 in ch2' '% ch2 in ch1' 'Thr ch1' 'Thr ch2'};
    ColData = [];
    % table all data
    uit = uitable(f, ...
        'Tag', 'uitAll', ...
        'Data', ColData, ...
        'ColumnName', ColNames, ...
        'Units', 'normalized', ...
        'Position', [0.54,0.058,0.33,0.26] ...
        );
end
